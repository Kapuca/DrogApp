{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "appData": {
    "version": 0.2
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.ts"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
	 {
      "name": "backend-data",
      "urls": [
        "https://drogapp.drogart.org//droge.php?type=json",
        "https://drogapp.drogart.org//nasveti.php?type=json",
        "https://drogapp.drogart.org//zapleti.php?type=json",
        "https://drogapp.drogart.org//emergency.php?type=json",
        "https://drogapp.drogart.org//info.php?type=json"
      ],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "5d",
        "strategy": "performance"
      }
    },
    {
      "name": "backend-feed",
      "urls": [
        "https://drogapp.drogart.org//opozorila.php?type=json"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "12h",
        "timeout": "0u",
        "strategy": "freshness"
      }
    }
  ]
}
