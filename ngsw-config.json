{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "version": 0.1,
  "index": "/index.html",
  "scope": "/DrogApp/",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
			 "/*.woff2",
			 "/assets/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
			 "/*.woff2",
			 "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
	 {
      "name": "drogapp.drogart.org",
      "urls": [
        "/droge.php?type=json",
        "/nasveti.php?type=json",
        "/zapleti.php?type=json",
        "/emergency.php?type=json",
        "/info.php?type=json",
        "/opozorila.php?type=json"
      ],
      "cacheConfig": {
        "maxSize": 7,
        "maxAge": "5d",
        "strategy": "performance"
      }
    }
  ]
}
