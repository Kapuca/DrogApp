{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "version": 0.1,
  "index": "/index.html",
  "scope": "/",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
			 "/*.woff2",
			 "/assets/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
			 "/*.woff2",
			 "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
	 {
      "name": "drogapp.drogart.org",
      "urls": [
        "https://drogapp.drogart.org/**"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "100d",
        "strategy": "performance"
      }
    }
  ]
}
